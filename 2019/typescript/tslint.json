{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single"],
      "no-console": false,
      "arrow-parens": [true, "ban-single-arg-parens"],
      "one-variable-per-declaration": false,
      "interface-name": false,
      "no-namespace": false,
      "max-classes-per-file": false,
      "no-use-before-declare": false
    },
    "rulesDirectory": []
}
